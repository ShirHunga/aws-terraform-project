{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "EKSPermissions",
      "Effect": "Allow",
      "Action": [
        "eks:*",
        "ec2:*",
        "autoscaling:*",
        "iam:PassRole",
        "iam:GetRole",
        "iam:CreateRole",
        "iam:DeleteRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:CreateInstanceProfile",
        "iam:AddRoleToInstanceProfile",
        "iam:RemoveRoleFromInstanceProfile",
        "iam:DeleteInstanceProfile",
        "iam:GetInstanceProfile"
      ],
      "Resource": "*"
    },
    {
      "Sid": "GeneralSupport",
      "Effect": "Allow",
      "Action": [
        "tag:*",
        "sts:GetCallerIdentity",
        "logs:*",
        "cloudwatch:*"
      ],
      "Resource": "*"
    }
  ]
}

